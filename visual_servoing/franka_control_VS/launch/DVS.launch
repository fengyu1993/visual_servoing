<?xml version="1.0" ?>
<launch>

<arg name="robot_ip" default="172.16.0.2" />
<arg name="load_gripper" default="true" />

<rosparam file= "$(find franka_control_VS)/param/parameters_VS.yaml" command= "load"/>

<!-- <include file="$(dirname)/franka_demo.launch" /> -->
<include file="$(dirname)/franka_control.launch" >
    <arg name="robot_ip" value="$(arg robot_ip)" />
</include>
<include file="$(dirname)/camera_calibration.launch" />

<include file="$(find franka_control_wrappers)/launch/cartesian_velocity_controller.launch">
    <arg name="stopped" value="true" />
</include>

<!-- <include file="$(find realsense2_camera)/launch/rs_aligned_depth.launch">
    <arg name="depth_width"              value="640"/>
    <arg name="depth_height"              value="480"/>
    <arg name="color_width"              value="640"/>
    <arg name="color_height"              value="480"/>
</include> -->
<node name="publish_realsense" pkg="franka_control_VS"  type="publish_realsense"  output="screen"/>

<node name="DVS" pkg="franka_control_VS"  type="franka_control_DVS"  output="screen"/>

</launch>