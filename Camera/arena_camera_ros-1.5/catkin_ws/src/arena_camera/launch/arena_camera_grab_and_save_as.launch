<?xml version="1.0"?>
<launch>
    <arg name="respawn" default="false" />
    <arg name="node_name" default="arena_camera_grab_and_save_as" />
    <arg name="camera_name" default="/arena_camera_node" />

    <node name="$(arg node_name)" pkg="arena_camera"
          type="grab_and_save_image_action_server.py" output="screen"
          respawn="$(arg respawn)">
        <param name="camera_name" value="$(arg camera_name)" type="string" />
    </node>
</launch>
