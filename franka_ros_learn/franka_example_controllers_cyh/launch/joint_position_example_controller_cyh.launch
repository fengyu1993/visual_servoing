<?xml version="1.0" ?>

<launch>
    <arg name="arm_id" default="panda" />

    <include file= "$(find franka_control)/launch/franka_control.launch" />

    <rosparam command="load" file="$(find franka_example_controllers_cyh)/config/franka_example_controllers.yaml" subst_value="true" />

    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen"  args="joint_position_example_controller_cyh"/>

    <node pkg="rviz" type="rviz" output="screen" name="rviz" args="-d $(find franka_example_controllers_cyh)/launch/robot.rviz"/>
</launch>